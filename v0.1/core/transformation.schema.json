  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Definition of a transformation between different spaces",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "@context": {
      "type": "string"
    },
    "transformationType": {
      "type": "string",
      "enum": ["linear", "non-linear"]
    },
    "uniqueId": {
      "type": "string"
    }
  }
}
