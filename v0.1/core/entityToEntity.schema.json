{
  "$id": "https://schema.hbp.eu/sand/0.1/core/entityToEntity.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Definition of a entityToEntity",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "@type": {
      "type": "string",
      "description": "Metadata node type (schema).",
      "const": "https://schema.hbp.eu/sands/EntityToEntity"
    },
    "@id": {
      "type": "string",
      "description": "Metadata node identifier.",
      "pattern": "sands/v0.1/core/entityToEntity/([A-Za-z0-9_-]+).json"
    },
    "performedBy": {
      "$ref": "https://raw.githubusercontent.com/HumanBrainProject/SANDS/master/v0.1/definitions.json#definitions/CONTROLLED_TERM",
      "description": "performedBy"
    },
    "relationType": {
      "$ref": "https://raw.githubusercontent.com/HumanBrainProject/SANDS/master/v0.1/definitions.json#definitions/RELATION_TYPE",
      "description": "relationType"
    },
    "confidence": {
      "type": "number",
      "description": "confidence"
    },
    "anatomicalEntitySubject":{
      "$ref": "https://raw.githubusercontent.com/HumanBrainProject/SANDS/master/v0.1/definitions.json#definitions/ANATOMICAL_ENTITY",
      "description": "anatomicalEntitySubject"
    },
    "anatomicalEntityObject": {
      "$ref": "https://raw.githubusercontent.com/HumanBrainProject/SANDS/master/v0.1/definitions.json#definitions/ANATOMICAL_ENTITY",
      "description": "anatomicalEntityObject"
    }
  }
}
